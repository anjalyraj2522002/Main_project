    <div class="container-lg">
        <div class="card">
            <div class="card-body">
                <h6 class="mb-4">Complaints</h6>
                <table class="w-100" id="dataTable">
                    <thead>
                        <tr>
                            <th>No.</th>
                            <th data-type="date" data-format="YYYY/DD/MM"> Date</th>
                            <th>Complaint id</th>
                            <th>subject</th>
                            <th>status</th>
                            <th>days</th>
                            <th>Manage</th>
                        </tr>
                    </thead>
                    <tbody>
                       {{#each cmp}}
                                    <tr>
                                        <th scope="row">{{incremented @index}}</th>
                                        <td class="date-cell">{{formatDate this.date}}</td>
                                        <td>{{this.complaintId}}</td>
                                        <td>{{this.subject}}</td>
                                        <td>{{this.status}}</td>
                                        <td>{{daysElapsed this.date}}</td>
                                        <td>
                                             <a class="btn btn-danger btn-sm" href="/govt/view-cmp/{{this._id}}"><i
                                                            class="bi bi-">manage</i></a>
                                             {{!-- <button type="submit" class="btn btn-danger btn-sm"><i
                                                            class="bi bi-">assign</i></button> --}}
                                        </td>
                                      
